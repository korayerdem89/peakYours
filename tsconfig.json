{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "strict": true,
      "allowJs": true,
      "esModuleInterop": true,
      "jsx": "react-native",
      "lib": ["DOM", "ESNext"],
      "moduleResolution": "node",
      "noEmit": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "target": "ESNext",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"],
        "@env": ["./node_modules/react-native-dotenv"]
      }
    },
    "include": ["**/*.ts", "**/*.tsx", "nativewind-env.d.ts"]
  }
  